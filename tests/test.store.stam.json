{
    "@type": "AnnotationStore",
    "@id": "Example A",
    "annotationsets": [
        {
            "@type": "AnnotationDataSet",
            "@id": "https://example.org/test/",
            "@include": "test.annotationset.stam.json"
        }
    ],
    "resources":  [{
        "@type": "TextResource",
        "@include": "hello.txt"
    }],
    "annotations": [
        {
            "@type": "Annotation",
            "@id": "A1",
            "data": [ {
                "@type": "AnnotationData",
                "@id": "PosInterjection",
                "set": "https://example.org/test/"
            }],
            "target": {
                "@type": "TextSelector",
                "resource": "hello.txt",
                "offset": {
                    "begin": {
                        "@type": "BeginAlignedCursor",
                        "value": 0
                    },
                    "end":  {
                        "@type": "BeginAlignedCursor",
                        "value": 5
                    }
                }
            }
        },
        {
            "@type": "Annotation",
            "@id": "A2",
            "data": [ {
                "@type": "AnnotationData",
                "@id": "PosNoun",
                "set": "https://example.org/test/"
            }],
            "target": {
                "@type": "TextSelector",
                "resource": "hello.txt",
                "offset": {
                    "begin": {
                        "@type": "BeginAlignedCursor",
                        "value": 6
                    },
                    "end":  {
                        "@type": "BeginAlignedCursor",
                        "value": 13
                    }
                }
            }
        }
    ]
}
